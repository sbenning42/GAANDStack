{
  "name": "gaand-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gql:introspect": "npx gql-gen --schema http://localhost:4266 --template graphql-codegen-introspection-template --out ./src/app/graphql/schema.json",
    "gql:generate": "npx gql-gen --schema ./src/app/graphql/schema.json --template graphql-codegen-apollo-angular-template --out src/app/graphql/index.ts src/**/*.graphql",
    "sync": "yarn run gql:introspect && yarn run gql:generate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "apollo-angular": "^1.5.0",
    "apollo-angular-link-http": "^1.6.0",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "core-js": "^2.5.4",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@graphql-codegen/cli": "^1.1.3",
    "@graphql-codegen/typescript": "^1.1.3",
    "@graphql-codegen/typescript-apollo-angular": "^1.1.3",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "graphql-code-generator": "^0.18.1",
    "graphql-codegen-apollo-angular-template": "^0.18.1",
    "graphql-codegen-introspection-template": "^0.18.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": ">=2.7.2 <2.10"
  }
}
